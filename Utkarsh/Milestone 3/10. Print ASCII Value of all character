#include <iostream>
using namespace std;

int main()
{
    int i;
    for(i=0; i<=255; i++)
    {
        cout<<"ASCII Value of character ";
        printf("%c = ",i);
        cout<<""<<i<<" \n";
    }
    return 0;
}
